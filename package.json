{
  "name": "cloudjs",
  "version": "1.0.0",
  "description": "Start your own cloud with Cloudjs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.js",
    "start": "node ./src/index.js",
    "compile": "pkg --debug src/index.js -c package.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "pkg": "^5.8.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }, 
  "pkg": {
    "scripts": "src/**/*.js",
    "targets": [ 
      "node14-linux", 
      "node14-linux-arm64", 
      "node14-macos", 
      "node14-win" 
    ],
    "outputPath": "dist"
  }
}
